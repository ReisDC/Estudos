	public class TestePalindromo {
	    public static void main(String[] args) {
	        ListaDupla lista = new ListaDupla();
	        lista.add('r');
	        lista.add('a');
	        lista.add('d');
	        lista.add('a');
	        lista.add('r');

	        lista.testPalindrome();
	    }
	}}
