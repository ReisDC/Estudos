package duplamente;

public class Testar_questao14 {
	
	public static void main(String[] args) {
        ListaDupla lista = new ListaDupla();
        lista.inserirOrdenado('r');
        lista.inserirOrdenado('a');
        lista.inserirOrdenado('d');
        lista.inserirOrdenado('a');
        lista.inserirOrdenado('r');

        lista.testPalindrome();
    }
	    }
